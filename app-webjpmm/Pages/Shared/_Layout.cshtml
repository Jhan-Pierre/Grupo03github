<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Code Creators</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/app_webjpmm.styles.css" asp-append-version="true" /> 
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/all.min.css">

    <script src="https://kit.fontawesome.com/eb496ab1a0.js" crossorigin="anonymous"></script>
</head>
<body>
    <a href="https://api.whatsapp.com/send?phone=+51941120990&text=Hola,quiero%20comenzar%20con%20mi%20negocio%20en%20linea" class="btn-wsp" target="_blank">
        <i class="fa fa-whatsapp icono"></i>
    </a>
    <header>
        <nav class="navbar navbar-expand-lg bg-body-tertiary border-bottom mx-auto" style="width: 70%;">
            <div class="container-fluid">
                <!-- Agrega el logo y el nombre de la empresa -->
                <a class="navbar-brand" href="#">
                    <img src="~/img/logo_6676F9.svg" alt="Logo">
                    <span class="text-black company-name">CodeCreators</span>
                </a>

                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse justify-content-center" id="navbarSupportedContent">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="text-black nav-link" asp-page="/Index">Inicio</a>
                        </li>
                        <li class="nav-item">
                            <a class="text-black nav-link" asp-page="/Nosotros">Nosotros</a>
                        </li>
                        <li class="nav-item">
                            <a class="text-black nav-link" asp-page="/Servicios">Servicios</a>
                        </li>
                        <li class="nav-item">
                            <a class="text-black nav-link" href="#">Contacto</a>
                        </li>
                    </ul>
                </div>
                <ul class="navbar-nav ml-auto img-links">
                    <li class="nav-item">
                        <a class="nav-link" href="https://www.facebook.com/fabian.ambrosio.796">
                            <i class="fab fa-facebook fa-2x" style="color: #000;"></i>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="https://www.facebook.com/fabian.ambrosio.796">
                            <i class="fab fa-twitter fa-2x" style="color: #000;"></i>
                        </a>
                    </li>
                </ul>
            </div>
        </nav>
    </header>


    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - CodeCreators - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>
    <script src="https://unpkg.com/scrollreveal"></script>
    <script src="https://unpkg.com/typed.js@2.0.16/dist/typed.umd.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>